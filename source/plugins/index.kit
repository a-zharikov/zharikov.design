<!DOCTYPE html>
<html lang="ru">
  <!-- $page-title = Плагины -->
  <!-- $page-description = jQuery и JavaScript плагины -->
  <!-- $page-keywords = простые и легкие плагины на JavaScript -->
  <!-- @import ../_kit/_layout/_head-page.kit -->
  <script src="../javascript/plugins/modal.min.js" type="text/javascript"></script>
  <body class="plugins">
    <!-- @import ../_kit/_layout/_header.kit -->
    <main class="page">
      <!-- @import ../_kit/_layout/_sidebar.kit -->
      <section class="page-feed">
        <div class="page-feed__header">
          <h1 class="page-feed__title"><!-- $page-title --></h1>
          <p class="page-feed__description"><!-- $page-description --></p>
        </div>
        <ul class="plugins__list">
          <li class="plugins__item">
            <a class="plugins__link js-open-modal" href="#select-page" data-url="select.html">
              <img class="plugins__cover __light" src="/images/plugins/select.svg" alt="Умный select">
              <img class="plugins__cover __dark" src="/images/plugins/select_dark.svg" alt="Умный select">
              <h2 class="plugins__title">Умный select</h2>
            </a>
          </li>
          <li class="plugins__item">
            <a class="plugins__link js-open-modal" href="#ancore-page" data-url="ancore.html">
              <img class="plugins__cover __light" src="/images/plugins/ancore.svg" alt="Якорное меню из заголовков">
              <img class="plugins__cover __dark" src="/images/plugins/ancore_dark.svg" alt="Якорное меню из заголовков">
              <h2 class="plugins__title">Якорное меню из заголовков</h2>
            </a>
          </li>
          <li class="plugins__item">
            <a class="plugins__link js-open-modal" href="#length-page" data-url="length.html">
              <img class="plugins__cover __light" src="/images/plugins/length.svg" alt="Подсчет слов в элементе">
              <img class="plugins__cover __dark" src="/images/plugins/length_dark.svg" alt="Подсчет слов в элементе">
              <h2 class="plugins__title">Подсчет слов в элементе</h2>
            </a>
          </li>
          <li class="plugins__item">
            <a class="plugins__link js-open-modal" href="#scroll-page" data-url="scroll.html">
              <img class="plugins__cover __light" src="/images/plugins/scroll.svg" alt="Добавляем класс элементу по скроллу страницы">
              <img class="plugins__cover __dark" src="/images/plugins/scroll_dark.svg" alt="Добавляем класс элементу по скроллу страницы">
              <h2 class="plugins__title">Добавляем класс элементу по скроллу страницы</h2>
            </a>
          </li>
          <li class="plugins__item">
            <a class="plugins__link js-open-modal" href="#table-page" data-url="table.html">
              <img class="plugins__cover __light" src="/images/plugins/table.svg" alt="Поиск по таблице и спискам">
              <img class="plugins__cover __dark" src="/images/plugins/table_dark.svg" alt="Поиск по таблице и спискам">
              <h2 class="plugins__title">Поиск по таблице и спискам</h2>
            </a>
          </li>
          <li class="plugins__item">
            <a class="plugins__link js-open-modal" href="#tabs-page" data-url="tabs.html">
              <img class="plugins__cover __light" src="/images/plugins/tabs.svg" alt="Простые и легкие табы">
              <img class="plugins__cover __dark" src="/images/plugins/tabs_dark.svg" alt="Простые и легкие табы">
              <h2 class="plugins__title">Простые и легкие табы</h2>
            </a>
          </li>
        </ul>
      </section>
    </main>
    <!-- @import ../_kit/_layout/_footer.kit -->
  </body>
</html>
